{
  "name": "shuttle-admin-rest-API",
  "version": "1.2.2",
  "description": "Express boilerplate for building RESTful APIs",
  "author": "Dev <devrajthapa1191@gmail.com>",
  "main": "src/index.js",
  "private": false,
  "license": "MIT",
  "engines": {
    "node": ">=18.20.0",
    "npm": ">=9"
  },
  "scripts": {
    "start": "cross-env TZ='Asia/Kolkata' NODE_ENV=production node ./src/index.js",
    "dev": "nodemon ./src/index.js",
    "start:workers": "cross-env TZ='Asia/Kolkata' NODE_ENV=production node src/api/workers/index.js",
    "lint": "eslint ./src/ --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:fix": "eslint ./src/ --fix",
    "lint:watch": "eslint ./src/ --watch",
    "test": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha --timeout 20000 --exit --recursive src/api/tests",
    "test:unit": "cross-env NODE_ENV=test mocha src/api/tests/unit",
    "test:integration": "cross-env NODE_ENV=test mocha --timeout 20000 --exit src/api/tests/integration",
    "test:watch": "cross-env NODE_ENV=test mocha --watch src/api/tests/unit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "postcoverage": "open-cli coverage/lcov-report/index.html",
    "validate": "eslint ./src && yarn test",
    "postpublish": "git push --tags",
    "docs": "apidoc -i src -o docs",
    "postdocs": "open-cli docs/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:danielfsousa/express-rest-es2017-boilerplate.git"
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "mongodb",
    "mongoose",
    "passport",
    "mocha",
    "nyc",
    "eslint",
    "REST",
    "API",
    "boilerplate",
    "starter project"
  ],
  "dependencies": {
    "@aws-sdk/client-s3": "^3.350.0",
    "@aws-sdk/s3-request-presigner": "^3.350.0",
    "@faker-js/faker": "^9.2.0",
    "@hilarion/mongoose-activity-logger": "^1.1.1",
    "apn": "^2.2.0",
    "aws-sdk": "^2.1691.0",
    "axios": "^1.7.7",
    "base64-img": "^1.0.4",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "body-parser": "^1.20.3",
    "bullmq": "^5.63.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv-safe": "^9.1.0",
    "email-templates": "^11.1.1",
    "express": "^4.21.1",
    "express-fileupload": "^1.5.1",
    "express-validation": "^4.1.1",
    "fcm-node": "^1.6.1",
    "firebase-admin": "^11.11.1",
    "helmet": "^8.0.0",
    "http-status": "^1.7.4",
    "ioredis": "^5.8.2",
    "joi": "^17.13.3",
    "jwt-simple": "^0.5.6",
    "lodash": "^4.17.21",
    "method-override": "^3.0.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.46",
    "mongoose": "^8.8.3",
    "mongoose-aggregate-paginate-v2": "^1.1.2",
    "mongoose-paginate-v2": "^1.8.5",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^3.3.7",
    "node-apn": "^3.0.0",
    "node-schedule": "^2.1.1",
    "nodemailer": "^6.9.16",
    "objectid-to-timestamp": "^1.3.0",
    "passport": "^0.7.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.1",
    "path": "^0.12.7",
    "pm2": "^5.4.2",
    "pug": "^3.0.3",
    "razorpay": "^2.9.4",
    "sharp": "^0.33.5",
    "slug": "^9.1.0",
    "uuid": "^11.0.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "apidoc": "^1.2.0",
    "chai": "^5.1.2",
    "chai-as-promised": "^8.0.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.31.0",
    "husky": "^9.1.7",
    "mocha": "^10.8.2",
    "nodemon": "^3.1.7",
    "nyc": "^17.1.0",
    "open-cli": "^8.0.0",
    "sinon": "^19.0.2",
    "sinon-chai": "^4.0.0",
    "supertest": "^7.0.0"
  }
}
